{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "module": "esnext",
    "strict": true,
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
